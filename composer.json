{
    "name": "seventyfourlaboratories/google-api-bundle",
    "description": "Integrates Google APIs Client Library for PHP with Symfony2",
    "keywords": ["google", "gdata"],
    "homepage": "http://github.com/SeventyFourLaboratories/SFLGoogleApiBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Seventy Four Laboratories",
        "homepage": "http://74labs.com"
    }],
    "require": {
        "symfony/framework-bundle": ">=2.2,<2.4-dev",
        "google/google-api-php-client": "0.6.7"
    },
    "autoload": {
        "psr-0": { "SFL\\GoogleApiBundle\\": "" }
    },
    "target-dir" : "SFL/GoogleApiBundle",
    "repositories": [
        {
            "type": "composer",
            "url": "http://raw.github.com/SeventyFourLaboratories/google-api-php-client/master/packages.json"
        }
    ]
}
